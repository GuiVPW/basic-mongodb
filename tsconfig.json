{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonjs",
		"lib": [
			"ESNext"
		],
		"moduleResolution": "node",
		"rootDir": ".",
		"baseUrl": "src",
		"noImplicitAny": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"esModuleInterop": true,
		"allowUmdGlobalAccess": true,
		"forceConsistentCasingInFileNames": true,
	}
}
